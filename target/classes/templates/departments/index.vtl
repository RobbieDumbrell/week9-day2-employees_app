<p class="pageHeader">All Departments</p>

<table>

    <tr>
        <th>Title</th>
    </tr>

    #foreach($department in $departments)
    <tr>
        <td>$department.getTitle()</td>
        <td><a href="/departments/$department.getId()">More</a></td>
    </tr>
    #end

</table>

<a href="/departments/new">Add Department</a>






